	/* tegra124-soc.dtsi enable all the i2c bus and HDMI, disable them again here
	   because not all baseboards will use them. */
	i2c@7000c000 {
		status = "disabled";
	};

	i2c@7000c400 {
		status = "disabled";
	};

	i2c@7000c500 {
		status = "disabled";
		pull-ups-supply = <&avdd_1v8_cam_run>;
	};

	i2c@7000c700 {
		status = "disabled";
	};

	i2c@7000d000 {
		status = "okay";

		/* Required for PMIC access via i2c during shutdown: */
		nvidia,bit-banging-xfer-after-shutdown;

		pmic: pmic@40 {
			compatible = "ams,as3722";
			reg = <0x40>;
			interrupts = <0 86 IRQ_TYPE_LEVEL_HIGH>;

			#interrupt-cells = <2>;
			interrupt-controller;

			gpio-controller;
			#gpio-cells = <2>;

			#address-cells = <1>;
			#size-cells = <0>;

			ams,major-rev = <1>;
			ams,minor-rev = <2>;
			ams,system-power-controller;
			ams,enable-ac-ok-power-on;
			ams,extcon-name = "as3722-extcon";
			ams,enable-adc1-continuous-mode;
			ams,enable-low-voltage-range;
			ams,adc-channel = <12>;
			ams,hi-threshold = <256>;
			ams,low-threshold = <128>;
			ams,enable-clock32k-out;

			pinctrl-names = "default";
			pinctrl-0 = <&as3722_default>;

			as3722_input: input@0 {
				status = "okay";

				compatible = "ams,as3722-input";
				reg = <0>;
			};

			as3722_adc: adc@1 {
				status = "okay";

				compatible = "ams,as3722-adc";
				reg = <1>;

				#io-channel-cells = <1>;
			};

			as3722_default: pinmux@0 {
				gpio0 {
					pins = "gpio0";
					function = "gpio";
					output-low;
				};

				gpio1_2 {
					pins = "gpio1", "gpio2";
					function = "gpio";
					bias-pull-down;
					output-high;
				};

				gpio7 {
					pins = "gpio7";
					function = "gpio";
					bias-pull-up;
					output-high;
				};

				gpio3 {
					pins = "gpio3";
					function = "gpio";
					bias-high-impedance;
				};
			};

			regulators {
				compatible = "ams,as3722-regulator";

				vsup-sd2-supply = <&vdd_5v0_sys>;
				vsup-sd3-supply = <&vdd_5v0_sys>;
				vsup-sd4-supply = <&vdd_5v0_sys>;
				vsup-sd5-supply = <&vdd_5v0_sys>;
				vin-ldo0-supply = <&vdd_1v35_lp0>;
				vin-ldo1-6-supply = <&vdd_3v3_run>;
				vin-ldo2-5-7-supply = <&vddio_1v8>;
				vin-ldo3-4-supply = <&vdd_3v3_sys>;
				vin-ldo9-10-supply = <&vdd_5v0_sys>;
				vin-ldo11-supply = <&vdd_3v3_run>;

				vdd_cpu: sd0 {
					regulator-name = "+VDD_CPU_AP";
					regulator-min-microvolt = <650000>;
					regulator-max-microvolt = <1400000>;
					regulator-min-microamp = <3500000>;
					regulator-max-microamp = <3500000>;
					regulator-always-on;
					regulator-boot-on;
					ams,ext-control = <AS3722_EXT_CONTROL_ENABLE2>;

					consumers {
						cpu {
							regulator-consumer-supply = "vdd_cpu";
						};
					};
				};

				sd1 {
					regulator-name = "+VDD_CORE";
					regulator-min-microvolt = <700000>;
					regulator-max-microvolt = <1350000>;
					regulator-min-microamp = <3500000>;
					regulator-max-microamp = <3500000>;
					regulator-always-on;
					regulator-boot-on;
					ams,ext-control = <AS3722_EXT_CONTROL_ENABLE1>;

					consumers {
						core {
							regulator-consumer-supply = "vdd_core";
						};
					};
				};

				vdd_1v35_lp0: sd2 {
					 regulator-name = "+1V35_LP0(sd2)";
					 regulator-always-on;
					 regulator-boot-on;
				 };

				sd3 {
					 regulator-name = "+1V35_LP0(sd3)";
					 regulator-always-on;
					 regulator-boot-on;
				 };

				vdd_1v05_run: sd4 {
					regulator-name = "+1V05_RUN";
					regulator-min-microvolt = <1050000>;
					regulator-max-microvolt = <1050000>;
					ams,ext-control = <AS3722_EXT_CONTROL_ENABLE1>;
				};

				vddio_1v8: sd5 {
					regulator-name = "+1V8_VDDIO";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
					regulator-always-on;
					regulator-boot-on;

					consumers {
						temp_sensor {
							regulator-consumer-device = "4-004c";
							regulator-consumer-supply = "vdd";
						};
					};
				};

				vdd_gpu: sd6 {
					regulator-name = "+VDD_GPU_AP";
					regulator-min-microvolt = <650000>;
					regulator-max-microvolt = <1200000>;
					regulator-min-microamp = <3500000>;
					regulator-max-microamp = <3500000>;
					regulator-init-microvolt = <1000000>;
					regulator-boot-on;
					/* CHECK in mainline also regulator-always-on */

					consumers {
						gpu {
							regulator-consumer-supply = "vdd_gpu";
						};
						gpu_simon {
							regulator-consumer-supply = "vdd_gpu_simon";
						};
					};
				};

				avdd_1v05_run: ldo0 {
					regulator-name = "+1V05_RUN_AVDD";
					regulator-min-microvolt = <1050000>;
					regulator-max-microvolt = <1050000>;
					regulator-always-on;
					regulator-boot-on;
					ams,ext-control = <AS3722_EXT_CONTROL_ENABLE1>;
				};

				avdd_1v8_cam_run: ldo1 {
					regulator-name = "+1V8_RUN_CAM";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
				};

				avdd_dsi_csi: ldo2 {
					regulator-name = "+1V2_GEN_AVDD";
					regulator-min-microvolt = <1200000>;
					regulator-max-microvolt = <1200000>;
					regulator-boot-on;
					/* CHECK in mainline also regulator-always-on */
					consumers {
						/* Second vi has no DT entry, provide reg supply here */
						c1 {
							regulator-consumer-supply = "avdd_dsi_csi";
							regulator-consumer-device = "vi.1";
						};
					};
				};

				ldo3 {
					regulator-name = "+1V05_LDO_VDD_RTC";
					regulator-min-microvolt = <1000000>;
					regulator-max-microvolt = <1000000>;
					regulator-always-on;
					regulator-boot-on;
					ams,enable-tracking;
					ams,disable-tracking-suspend;
				};

				avdd_lvds: ldo4 {
					regulator-name = "AVDD_LVDS_PLL";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <3300000>;
				};

				vddio_sdmmc3: ldo6 {
					regulator-name = "+VDDIO_SDMMC3";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <3300000>;
				};

				vpp_fuse: ldo11 {
					regulator-name = "+1V8_RUN_VPP_FUSE";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
				};

			};
		};

		com_temperature_sensor: temperature-sensor@4c {
			compatible = "tmp451";
			reg = <0x4c>;
			interrupt-parent = <&gpio>;
			interrupts = <TEGRA_GPIO(I, 6) IRQ_TYPE_LEVEL_LOW>;
			vcc-supply = <&vddio_1v8>;

			#thermal-sensor-cells = <1>;
		};
	};

	i2c@7000d100 {
		status = "disabled";
	};

	spi@7000d400 {
		status = "disabled";
	};

	spi@7000d600 {
		status = "disabled";
	};

	spi@7000d800 {
		status = "disabled";
	};

	spi@7000da00 {
		status = "disabled";
	};

	spi@7000dc00 {
		status = "disabled";
	};

	spi@7000de00 {
		status = "disabled";
	};

	rtc {
		status = "okay";
	};

	pmc { /* The PMC is missing an address */
		status = "okay";
		nvidia,invert-interrupt;
		nvidia,suspend-mode = <0>;
		nvidia,cpu-pwr-good-time = <500>;
		nvidia,cpu-pwr-off-time = <300>;
		nvidia,core-pwr-good-time = <3845 3845>;
		nvidia,core-pwr-off-time = <2000>;
		nvidia,core-power-req-active-high;
		nvidia,sys-clock-req-active-high;
	};

	emc: memory-controller@7001b000 {
		compatible = "nvidia,tegra12-emc";
		reg = <0x0 0x7001b000 0x0 0x800>;
		#address-cells = <1>;
		#size-cells = <0>;
		nvidia,use-ram-code;
	};

	sata@0x70020000 {
		nvidia,enable-sata-port;
		nvidia,sata-connector-type = <STANDARD_SATA>;
		avdd_sata_pll-supply =<&vdd_1v05_run>;
		avdd_sata-supply = <&vdd_1v05_run>;
		hvdd_sata-supply = <&vdd_3v3_lp0>;
		vddio_pex_sata-supply = <&vdd_1v05_run>;
	};

	efuse@7000f800 {
		vpp_fuse-supply = <&vpp_fuse>;
	};

	/* Extra SD card / SDIO */
	sdhci@700b0000 {
		wp-gpios = <&gpio TEGRA_GPIO(V, 3) GPIO_ACTIVE_HIGH>;
		bus-width = <4>;
		vddio_sdmmc-supply = <&vddio_1v8>;

		#address-cells = <1>;
		#size-cells = <0>;
	};

	/* SD card */
	sdhci@700b0400 {
		cd-gpios = <&gpio TEGRA_GPIO(V, 2) GPIO_ACTIVE_LOW>;
		wp-gpios = <&gpio TEGRA_GPIO(Q, 4) GPIO_ACTIVE_HIGH>;
		bus-width = <4>;
		vddio_sdmmc-supply = <&vddio_sdmmc3>;

		#address-cells = <1>;
		#size-cells = <0>;
	};

	/* eMMC */
	sdhci@700b0600 {
		status = "okay";
		bus-width = <8>;
		built-in;

		tap-delay = <4>;
		trim-delay = <4>;
		ddr-clk-limit = <51000000>;
		max-clk-limit = <200000000>;
		calib-3v3-offsets = <0x0202>;
		calib-1v8-offsets = <0x0202>;
		mmc-ocr-mask = <0>;

		vddio_sdmmc-supply = <&vddio_1v8>;
		vddio_sd_slot-supply = <&vdd_3v3_run>;
	};

	tegra_ehci_0: usb@7d000000 {
		nvidia,phy-interface = <TEGRA_USB_PHY_INTF_UTMI>;
		nvidia,mode = <TEGRA_USB_OPMODE_HOST>;
		nvidia,id-det-type = <TEGRA_USB_PMU_ID>;
		nvidia,id-extcon-dev-name = "as3722-extcon";

		nvidia,hssync-start-delay = <0>;
		nvidia,elastic-limit = <16>;
		nvidia,idle-wait-delay = <17>;
		nvidia,term-range-adj = <6>;
		nvidia,xcvr-setup = <8>;
		nvidia,xcvr-lsfslew = <2>;
		nvidia,xcvr-lsrslew = <2>;
		nvidia,xcvr-setup-offset = <0>;
		nvidia,xcvr-use-fuses = <1>;

		avdd_usb-supply = <&vdd_3v3_lp0>;
		avdd_pll_utmip-supply = <&vddio_1v8>;
	};

	tegra_udc: udc@7d000000 {
		nvidia,port-otg;
		nvidia,phy-interface = <TEGRA_USB_PHY_INTF_UTMI>;
		nvidia,mode = <TEGRA_USB_OPMODE_DEVICE>;
		nvidia,id-det-type = <TEGRA_USB_PMU_ID>;
		nvidia,id-extcon-dev-name = "as3722-extcon";

		nvidia,hssync-start-delay = <0>;
		nvidia,elastic-limit = <16>;
		nvidia,idle-wait-delay = <17>;
		nvidia,term-range-adj = <6>;
		nvidia,xcvr-setup = <8>;
		nvidia,xcvr-lsfslew = <2>;
		nvidia,xcvr-lsrslew = <2>;
		nvidia,xcvr-setup-offset = <0>;
		nvidia,xcvr-use-fuses = <1>;

		avdd_usb-supply = <&vdd_3v3_lp0>;
		avdd_pll_utmip-supply = <&vddio_1v8>;
	};

	otg@7d000000 {
		nvidia,ehci-device = <&tegra_ehci_0>;
	};

	usb@7d004000 {
		nvidia,phy-interface = <TEGRA_USB_PHY_INTF_UTMI>;
		nvidia,mode = <TEGRA_USB_OPMODE_HOST>;

		nvidia,hssync-start-delay = <0>;
		nvidia,elastic-limit = <16>;
		nvidia,idle-wait-delay = <17>;
		nvidia,term-range-adj = <6>;
		nvidia,xcvr-setup = <8>;
		nvidia,xcvr-lsfslew = <2>;
		nvidia,xcvr-lsrslew = <2>;
		nvidia,xcvr-setup-offset = <0>;
		nvidia,xcvr-use-fuses = <1>;

		avdd_usb-supply = <&vdd_3v3_lp0>;
		avdd_pll_utmip-supply = <&vddio_1v8>;
	};

	usb@7d008000 {
		nvidia,phy-interface = <TEGRA_USB_PHY_INTF_UTMI>;
		nvidia,mode = <TEGRA_USB_OPMODE_HOST>;

		nvidia,hssync-start-delay = <0>;
		nvidia,elastic-limit = <16>;
		nvidia,idle-wait-delay = <17>;
		nvidia,term-range-adj = <6>;
		nvidia,xcvr-setup = <8>;
		nvidia,xcvr-lsfslew = <2>;
		nvidia,xcvr-lsrslew = <2>;
		nvidia,xcvr-setup-offset = <0>;
		nvidia,xcvr-use-fuses = <1>;

		avdd_usb-supply = <&vdd_3v3_lp0>;
		avdd_pll_utmip-supply = <&vddio_1v8>;
	};

	xusb@70090000 {
		hvdd_usb-supply = <&vdd_3v3_lp0>;
		avdd_pll_utmip-supply = <&vddio_1v8>;
		vddio_hsic-supply = <&avdd_dsi_csi>;
		avddio_usb-supply= <&avdd_1v05_run>;
	};

	dfll@70110000 {
		status = "okay";

		board-params = <&cl_dvfs_cfg_param>;
		i2c-pmic-integration = <&cl_dvfs_pmu_i2c>;
	};

	/* For the base board regulators, they should be probed first to avoid
	 * having probe deferals on PMIC regulators. For this they should appear
	 * first in the DTB, so we create the node here. */
	regulators {
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <0>;
	};

	com_regulators {
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <0>;

		vdd_mux: regulator@0 {
			compatible = "regulator-fixed";
			reg = <0>;
			regulator-name = "+VDD_MUX";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			regulator-always-on;
			regulator-boot-on;
			vin-supply = <&vdd_5v0>;
		};

		vdd_5v0_sys: regulator@1 {
			compatible = "regulator-fixed";
			reg = <1>;
			regulator-name = "+5V_SYS";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			regulator-always-on;
			regulator-boot-on;
			vin-supply = <&vdd_mux>;
		};

		vdd_3v3_sys: regulator@2 {
			compatible = "regulator-fixed";
			reg = <2>;
			regulator-name = "+3V3_SYS";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-always-on;
			regulator-boot-on;
			vin-supply = <&vdd_3v3>;
		};

		vdd_3v3_run: regulator@3 {
			compatible = "regulator-fixed";
			reg = <3>;
			regulator-name = "+3V3_RUN";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-always-on;
			regulator-boot-on;
			gpio = <&pmic 1 GPIO_ACTIVE_HIGH>;
			enable-active-high;
			vin-supply = <&vdd_3v3_sys>;
		};

		vdd_3v3_hdmi: regulator@4 {
			compatible = "regulator-fixed";
			reg = <4>;
			regulator-name = "+3V3_AVDD_HDMI_AP_GATED";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			vin-supply = <&vdd_3v3_run>;
		};

		vdd_3v3_lp0: regulator@5 {
			compatible = "regulator-fixed";
			reg = <5>;
			regulator-name = "+3V3_LP0";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-always-on;
			regulator-boot-on;
			gpio = <&pmic 2 GPIO_ACTIVE_HIGH>;
			enable-active-high;
			vin-supply = <&vdd_3v3_sys>;
		};

		vdd_hdmi_pll: regulator@6 {
			compatible = "regulator-fixed";
			reg = <6>;
			regulator-name = "+1V05_RUN_AVDD_HDMI_PLL_AP_GATED";
			regulator-min-microvolt = <1050000>;
			regulator-max-microvolt = <1050000>;
			gpio = <&gpio TEGRA_GPIO(H, 7) GPIO_ACTIVE_LOW>;
			vin-supply = <&vdd_1v05_run>;
		};
	};

	cpu_fan: pwm-fan {
		compatible = "pwm-fan";
		status = "disabled";
		pwms = <&tegra_pwm 0 40000>;

		#cooling-cells = <2>;
		cooling-levels = <0>, <128>, <192>, <255>;
	};

	iio_hwmon {
		compatible = "iio-hwmon";

		io-channels = <&as3722_adc AS3722_ADC_CHANNEL_TEMP_SENSOR>,
			<&as3722_adc AS3722_ADC_CHANNEL_VSUP>,
			<&as3722_adc AS3722_ADC_CHANNEL_VBAT>,
			<&as3722_adc AS3722_ADC_CHANNEL_TEMP1_SD0>,
			<&as3722_adc AS3722_ADC_CHANNEL_TEMP2_SD0>,
			<&as3722_adc AS3722_ADC_CHANNEL_TEMP_SD1>,
			<&as3722_adc AS3722_ADC_CHANNEL_TEMP1_SD6>,
			<&as3722_adc AS3722_ADC_CHANNEL_TEMP2_SD6>;
	};

	thermal-zones {
		com_thermal: com-thermal {
			polling-delay-passive = <250>;
			polling-delay = <1000>;

			thermal-sensors = <&com_temperature_sensor 0>;

			trips {
				com-crit {
					temperature = <120000>;
					hysteresis = <1000>;
					type = "critical";
				};
			};
		};

		cpu_thermal: cpu-thermal {
			polling-delay-passive = <250>;
			polling-delay = <1000>;

			thermal-sensors = <&com_temperature_sensor 1>;

			trips {
				cpu_alert0: cpu-alert0 {
					temperature = <25000>;
					hysteresis = <1000>;
					type = "active";
				};
				cpu_alert1: cpu-alert1 {
					temperature = <60000>;
					hysteresis = <1000>;
					type = "active";
				};
				cpu_alert2: cpu-alert2 {
					temperature = <80000>;
					hysteresis = <1000>;
					type = "active";
				};
				cpu_crit: cpu-crit {
					temperature = <95000>;
					hysteresis = <1000>;
					type = "critical";
				};
			};

			cooling-maps {
				map0 {
					trip = <&cpu_alert0>;
					cooling-device = <&cpu_fan 1 1>;
				};
				map1 {
					trip = <&cpu_alert1>;
					cooling-device = <&cpu_fan 1 2>;
				};
				map2 {
					trip = <&cpu_alert2>;
					cooling-device = <&cpu_fan 2 THERMAL_NO_LIMIT>;
				};

			};
		};
	};

	/* HACK driver for the power detection */
	power_detect {
		compatible = "nvidia,tegra124-power-detect";
		pwrdet_nand-supply = <&vddio_1v8>;
		pwrdet_uart-supply = <&vddio_1v8>;
		pwrdet_bb-supply = <&vddio_1v8>;
		pwrdet_audio-supply = <&vddio_1v8>;
		pwrdet_mipi-supply = <&avdd_dsi_csi>;
		pwrdet_cam-supply = <&avdd_1v8_cam_run>;
		pwrdet_pex_ctl-supply = <&vdd_1v05_run>;
		pwrdet_sdmmc1-supply = <&vddio_1v8>;
		pwrdet_sdmmc3-supply = <&vddio_sdmmc3>;
		pwrdet_sdmmc4-supply = <&vddio_1v8>;
		pwrdet_hv-supply = <&vdd_3v3_lp0>;
	};
};

/* Include the memory configs, these must be afterwards to
 * allow using the 'emc' label. */
#include "tegra124-meerkat-emc.dtsi"
