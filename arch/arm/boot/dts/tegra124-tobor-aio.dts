/dts-v1/;

#include <dt-bindings/display/tegra-dc.h>
#include <dt-bindings/usb/tegra-ehci.h>
#include <dt-bindings/usb/tegra-xhci.h>
#include <dt-bindings/pci/tegra-pcie.h>

#include "tegra124-meerkat.dtsi"
#include "panel-n-720-1280-5.dtsi"

/ {
	model = "Avionic Design Tobor All In One";
	compatible = "ad,tobor-aio", "ad,meerkat", "nvidia,tegra124";
	nvidia,dtsfilename = __FILE__;

	pinmux {
		common {
			uart3_rxd_pw7 {
				nvidia,tristate = <TEGRA_PIN_DISABLE>;
			};

			uart3_txd_pw6 {
				nvidia,tristate = <TEGRA_PIN_DISABLE>;
			};

			/* SPI2 */
			kb_row13_ps5 {
				nvidia,function = "spi2";
				nvidia,enable-input = <TEGRA_PIN_DISABLE>;
			};

			kb_row14_ps6 {
				nvidia,function = "spi2";
				nvidia,tristate = <TEGRA_PIN_DISABLE>;
			};

			kb_col0_pq0 {
				nvidia,function = "spi2";
				nvidia,enable-input = <TEGRA_PIN_DISABLE>;
				nvidia,pull = <TEGRA_PIN_PULL_NONE>;
			};

			kb_col1_pq1 {
				nvidia,function = "spi2";
				nvidia,tristate = <TEGRA_PIN_DISABLE>;
			};

			/* LCD_BL_PWM */
			ph3 {
				nvidia,function = "pwm3";
				nvidia,tristate = <TEGRA_PIN_DISABLE>;
			};

			/* i2s1 (DAP2) / ADC */
			dap2_din_pa4 {
				nvidia,tristate = <TEGRA_PIN_DISABLE>;
			};

			dap2_fs_pa2 {
				nvidia,tristate = <TEGRA_PIN_DISABLE>;
			};

			dap2_sclk_pa3 {
				nvidia,tristate = <TEGRA_PIN_DISABLE>;
			};

			/* i2s3 (DAP4) / DAC */
			dap4_dout_pp6 {
				nvidia,tristate = <TEGRA_PIN_DISABLE>;
			};
			dap4_fs_pp4 {
				nvidia,tristate = <TEGRA_PIN_DISABLE>;
			};
			dap4_sclk_pp7 {
				nvidia,tristate = <TEGRA_PIN_DISABLE>;
			};

			/* TI Wifi IRQ line */
			clk2_req_pcc5 {
				nvidia,enable-input = <TEGRA_PIN_ENABLE>;
				nvidia,pull = <TEGRA_PIN_PULL_NONE>;
				nvidia,tristate = <TEGRA_PIN_DISABLE>;
			};
		};
	};

	display_bl: backlight {
		compatible = "pwm-backlight";
		pwms = <&tegra_pwm 3 1000000>;
		brightness-levels = <0 4 8 16 32 64 128 255>;
		default-brightness-level = <7>;
	};

	host1x {
		/* DSI Panel */
		/* tegradc.0 */
		dc@54200000 {
			status = "okay";
			nvidia,dc-connection = "internal-lcd";
			nvidia,cmu-enable = <1>;
			panel = <&panel>;
		};

		dsi {
			nvidia,dsi-controller-vs = <DSI_VS_1>;
			status = "okay";
			panel: panel-n-720-1280-5 {
				status = "okay";
				enable-gpios = <&gpio TEGRA_GPIO(R, 2) GPIO_ACTIVE_HIGH>;
				reset-gpios = <&gpio TEGRA_GPIO(W, 2) GPIO_ACTIVE_LOW>;
				reset-hold = <20>;
				reset-settle = <40>;
				backlight = <&display_bl>;
			};
		};
	};

	/* Use HS UART for the first 3 ports */
	serial@70006000 {
		compatible = "nvidia,tegra114-hsuart", "nvidia,tegra30-hsuart";
		status = "okay";
	};

	serial@70006040 {
		compatible = "nvidia,tegra114-hsuart", "nvidia,tegra30-hsuart";
		status = "okay";
	};

	serial@70006200 {
		compatible = "nvidia,tegra114-hsuart", "nvidia,tegra30-hsuart";
		status = "okay";
	};

	serial@70006300 {
		status = "okay";
	};

	/* GEN 1 */
	i2c@7000c000 {
		status = "okay";

		tlv320dac3100: tlv320dac3100@18 {
			compatible = "ti,tlv320dac3100";
			reg = <0x18>;

			reset-gpio = <&gpio TEGRA_GPIO(R, 5) GPIO_ACTIVE_LOW>;
		};

		adc1: adc@48 {
			compatible = "cirrus,cs53l30";
			reg = <0x48>;

			#sound-dai-cells = <0>;
			reset-gpio = <&gpio TEGRA_GPIO(X, 6) GPIO_ACTIVE_LOW>;
		};

		adc2: adc@49 {
			compatible = "cirrus,cs53l30";
			reg = <0x49>;

			#sound-dai-cells = <0>;
			reset-gpio = <&gpio TEGRA_GPIO(X, 5) GPIO_ACTIVE_LOW>;
		};
	};

	/* GEN 2 */
	i2c@7000c400 {
		status = "okay";

		tmp103@70 {
			compatible = "ti,tmp103";
			reg = <0x70>;
		};
	};

	/* SPI2 */
	spi@7000d600 {
		status = "okay";
		spi-max-frequency = <8000000>;

		spi@0 {
			compatible = "invensense,icm20608";
			reg = <0>;

			spi-max-frequency = <8000000>;
		};
	};

	/* Limit MMC3 to 1.8V for TI SDIO */
	i2c@7000d000 {
		pmic: pmic@40 {
			regulators {
				vddio_sdmmc3: ldo6 {
					regulator-max-microvolt = <1800000>;
				};
			};

			/* Generate 32kHz clock for TI wifi module */
			as3722_default: pinmux@0 {
				gpio5 {
					pins = "gpio5";
					function = "clk32k-out";
				};
			};
		};
	};

	/* SPI4 */
	spi@7000da00 {
		status = "okay";
		spi-max-frequency = <25000000>;

		w25q64@0 {
			compatible = "w25q64fw";
			reg = <0>;
			spi-max-frequency = <25000000>;
		};
	};

	pcie-controller {
		nvidia,port0_status = <1>;
		nvidia,port1_status = <0>;
		nvidia,lanes = <TEGRA_PCIE_LANES_X2_X1>;
		status = "okay";
	};

	/* MMC card slot */
	sdhci@700b0400 {
		status = "okay";
		/delete-property/ cd-gpios;
		/delete-property/ wp-gpios;

		cap-power-off-card;
		keep-power-in-suspend;
		mmc-ocr-mask = <0>;
		vddio_sd_slot-supply = <&vdd_3v3_sd>;
		vmmc-supply = <&wlan_enable>;

		wlcore: wlcore@2 {
			status = "okay";
			compatible = "ti,wl1837";
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <2>;
			interrupt-parent = <&gpio>;
			interrupts = <TEGRA_GPIO(CC, 5) IRQ_TYPE_LEVEL_HIGH>;
		};
	};

	/* Recovery USB */
	tegra_ehci_0: usb@7d000000 {
		status = "okay";
		nvidia,port-otg;
		nvidia,phy-interface = <TEGRA_USB_PHY_INTF_UTMI>;
		nvidia,mode = <TEGRA_USB_OPMODE_HOST>;
		nvidia,id-det-type = <TEGRA_USB_PMU_ID>;
		nvidia,id-extcon-dev-name = "as3722-extcon";

		nvidia,hotplug;
		nvidia,remote-wakeup-supported;
		nvidia,power-off-on-suspend;

		nvidia,hssync-start-delay = <0>;
		nvidia,elastic-limit = <16>;
		nvidia,idle-wait-delay = <17>;
		nvidia,term-range-adj = <6>;
		nvidia,xcvr-setup = <8>;
		nvidia,xcvr-lsfslew = <2>;
		nvidia,xcvr-lsrslew = <2>;
		nvidia,xcvr-setup-offset = <0>;
		nvidia,xcvr-use-fuses = <1>;
		nvidia,vbus-oc-map = <0x5>;

		usb_vbus-supply = <&vdd_vbus_client>;
	};

	tegra_udc: udc@7d000000 {
		status = "okay";
		nvidia,port-otg;
		nvidia,has-host;
		nvidia,phy-interface = <TEGRA_USB_PHY_INTF_UTMI>;
		nvidia,mode = <TEGRA_USB_OPMODE_DEVICE>;
		nvidia,id-det-type = <TEGRA_USB_PMU_ID>;
		nvidia,id-extcon-dev-name = "as3722-extcon";

		nvidia,hssync-start-delay = <0>;
		nvidia,elastic-limit = <16>;
		nvidia,idle-wait-delay = <17>;
		nvidia,term-range-adj = <6>;
		nvidia,xcvr-setup = <8>;
		nvidia,xcvr-lsfslew = <2>;
		nvidia,xcvr-lsrslew = <2>;
		nvidia,xcvr-setup-offset = <0>;
		nvidia,xcvr-use-fuses = <1>;

		usb_vbus-supply = <&vdd_vbus_client>;
	};

	otg@7d000000 {
		status = "okay";
		nvidia,ehci-device = <&tegra_ehci_0>;

		usb_vbus-supply = <&vdd_vbus_client>;
	};

	/* USB1 */
	usb@7d004000 {
		status = "okay";
		nvidia,phy-interface = <TEGRA_USB_PHY_INTF_UTMI>;
		nvidia,mode = <TEGRA_USB_OPMODE_HOST>;

		nvidia,remote-wakeup-supported;
		nvidia,power-off-on-suspend;

		nvidia,hssync-start-delay = <0>;
		nvidia,elastic-limit = <16>;
		nvidia,idle-wait-delay = <17>;
		nvidia,term-range-adj = <6>;
		nvidia,xcvr-setup = <8>;
		nvidia,xcvr-lsfslew = <2>;
		nvidia,xcvr-lsrslew = <2>;
		nvidia,xcvr-setup-offset = <0>;
		nvidia,xcvr-use-fuses = <1>;
		nvidia,vbus-oc-map = <0x5>;

		usb_vbus-supply = <&vdd_vbus_usb1>;
	};

	/* USB2 */
	usb@7d008000 {
		status = "okay";
		nvidia,phy-interface = <TEGRA_USB_PHY_INTF_UTMI>;
		nvidia,mode = <TEGRA_USB_OPMODE_HOST>;

		nvidia,remote-wakeup-supported;
		nvidia,power-off-on-suspend;

		nvidia,hssync-start-delay = <0>;
		nvidia,elastic-limit = <16>;
		nvidia,idle-wait-delay = <17>;
		nvidia,term-range-adj = <6>;
		nvidia,xcvr-setup = <8>;
		nvidia,xcvr-lsfslew = <2>;
		nvidia,xcvr-lsrslew = <2>;
		nvidia,xcvr-setup-offset = <0>;
		nvidia,xcvr-use-fuses = <1>;
		nvidia,vbus-oc-map = <0x5>;
	};

	avp-audio {
		compatible = "nvidia,tegra124-avp-audio", "nvidia,tegra30-avp-audio";
	};

	audio-offload {
		compatible = "nvidia,tegra-offload";
	};

	ahub {
		status = "okay";

		i2s@70301100 {
			status = "okay";
		};

		i2s@70301300 {
			status = "okay";
		};
	};

	sound@0 {
		compatible = "nvidia,tegra-audio-tlv320dac3100";
		reg = <0x0 0x0 0x0 0x0>;
		nvidia,model = "Tobor TLV320DAC3100";

		nvidia,audio-routing =
			"Headphone", "Amp",
			"Headphone", "HPL",
			"Headphone", "HPR";

		nvidia,memory-clients = <SWGID_PPCS>;
		nvidia,i2s-controller = <&tegra_i2s3>;
		nvidia,i2s-is-master;
		nvidia,audio-codec = <&tlv320dac3100>;
		nvidia,extern-clk-id = <3>;

		Amp-supply = <&amp_enable>;
	};

	sound@1 {
		compatible = "ad,tobor-adc";
		reg = <0x0 0x1 0x0 0x0>;
		nvidia,model = "Tobor ADC";

		nvidia,audio-routing =
			"A IN1", "Mic 1",
			"A IN2", "Mic 2",
			"A IN3", "Mic 3",
			"A IN4", "Mic 4",
			"B IN1", "Mic 5",
			"B IN2", "Mic 6",
			"B IN3", "Loopback",
			"B IN4", "Loopback";

		nvidia,memory-clients = <SWGID_PPCS>;
		nvidia,i2s-controller = <&tegra_i2s1>;
		nvidia,i2s-is-master;
		sound-dai = <&adc1>, <&adc2>;

		adc-enable-gpio = <&gpio TEGRA_GPIO(T, 0) GPIO_ACTIVE_LOW>;
	};

	regulators {
		vsys: regulator@50 {
			compatible = "regulator-fixed";
			reg = <50>;
			regulator-name = "VSYS";
			regulator-min-microvolt = <12000000>;
			regulator-max-microvolt = <12000000>;
			regulator-always-on;
			regulator-boot-on;
		};

		vdd_5v0: regulator@101 {
			compatible = "regulator-fixed";
			reg = <101>;
			regulator-name = "+5V_SOM";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			vin-supply = <&vsys>;
		};

		vdd_3v3: regulator@102 {
			compatible = "regulator-fixed";
			reg = <102>;
			regulator-name = "+3V3_SOM";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			vin-supply = <&vsys>;
		};

		vdd_5v0_ext: regulator@103 {
			compatible = "regulator-fixed";
			reg = <103>;
			regulator-name = "+5V_EXT";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			vin-supply = <&vsys>;
		};

		vdd_3v3_ext: regulator@104 {
			compatible = "regulator-fixed";
			reg = <104>;
			regulator-name = "+3V3_EXT";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			vin-supply = <&vsys>;
		};

		vdd_1v8_ext: regulator@105 {
			compatible = "regulator-fixed";
			reg = <105>;
			regulator-name = "+1V8_EXT";
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
			vin-supply = <&vddio_1v8>;
		};

		vdd_5v0_hdmi: regulator@106 {
			compatible = "regulator-fixed";
			reg = <106>;
			regulator-name = "HDMI_VCC";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			vin-supply = <&vdd_5v0_ext>;
			gpio = <&gpio TEGRA_GPIO(H, 1) GPIO_ACTIVE_HIGH>;
			enable-active-high;
		};

		vdd_3v3_sd: regulator@107 {
			compatible = "regulator-fixed";
			reg = <107>;
			regulator-name = "+3V3_SD";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			vin-supply = <&vdd_3v3_ext>;
			gpio = <&gpio TEGRA_GPIO(R, 0) GPIO_ACTIVE_HIGH>;
			enable-active-high;
		};

		vdd_1v5_mpcie: regulator@108 {
			compatible = "regulator-fixed";
			reg = <108>;
			regulator-name = "+1V5_mPCIe";
			regulator-min-microvolt = <1500000>;
			regulator-max-microvolt = <1500000>;
			vin-supply = <&vdd_3v3_ext>;
		};

		vdd_3v3_mpcie: regulator@109 {
			compatible = "regulator-fixed";
			reg = <109>;
			regulator-name = "+3V3_mPCIe";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			vin-supply = <&vdd_3v3_ext>;
		};

		vdd_vbus_client: regulator@110 {
			compatible = "regulator-fixed";
			reg = <110>;
			regulator-name = "USB_CLIENT_VBUS";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			vin-supply = <&vdd_5v0_ext>;
			gpio = <&gpio TEGRA_GPIO(N, 4) GPIO_ACTIVE_HIGH>;
			enable-active-high;
			gpio-open-drain;
		};

		vdd_vbus_usb1: regulator@111 {
			compatible = "regulator-fixed";
			reg = <111>;
			regulator-name = "USB1_VCC";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			vin-supply = <&vdd_5v0_ext>;
			gpio = <&gpio TEGRA_GPIO(N, 5) GPIO_ACTIVE_HIGH>;
			enable-active-high;
			gpio-open-drain;
		};

		amp_enable: regulator@112 {
			compatible = "regulator-fixed";
			reg = <112>;
			regulator-name = "AMP_EN";
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
			vin-supply = <&vdd_5v0_ext>;
			gpio = <&gpio TEGRA_GPIO(S, 0) GPIO_ACTIVE_HIGH>;
			enable-active-high;
		};

		wlan_enable: regulator@113 {
			compatible = "regulator-fixed";
			reg = <113>;
			regulator-name = "WLAN_EN";
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
			vin-supply = <&vdd_5v0_ext>;
			gpio = <&gpio TEGRA_GPIO(J, 0) GPIO_ACTIVE_HIGH>;
			enable-active-high;
		};
	};

	pwm-fan {
		status = "okay";
	};
};
